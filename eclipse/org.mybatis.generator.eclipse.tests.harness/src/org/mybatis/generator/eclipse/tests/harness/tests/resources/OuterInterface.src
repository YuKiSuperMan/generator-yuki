package mbg.torture.test;

import java.io.Serializable;

public interface OuterInterface extends Serializable {
    static final String MY_NAME = OuterInterface.class.getName();
    
    void doSomething(int index);
    
    default int doSomethingElse() {
        return 3;
    }

    public enum InnerEnum {
        GEORGE(1), JANE(2);

        private int index;

        private InnerEnum(int index) {
            this.index = index;
        }

        public int getIndex() {
            return index;
        }
    }

    public class InnerClass {
        private String description;

        public String getDescription() {
            return description;
        }

        public void setDescription(String description) {
            this.description = description;
        }

    }
    
    public interface InnerInterface {
        void execute();
    }

    public @interface InnerAnnotation {
        String amount();
    }
}

interface SecondOuterInterface {
    void anotherMethod();
}
